Meteor.startup(function() {
	console.log("server")
	console.log(Meteor.users.find().count())
	if (Meteor.users.find().count() === 0) {
		Accounts.createUser({
			username: "admin",
			email: "jon@freaksauce.com",
			password: "password",
			profile: {
				first_name: "Jon",
				last_name: "Bloomer"
			}
		})
	}

});